{
  "configurations": [
    {
      "type": "aws-sam",
      "request": "direct-invoke",
      "name": "ezaudit-lambda:ezaudit-lambda",
      "invokeTarget": {
        "target": "template",
        "templatePath": "${workspaceFolder}/template.yaml",
        "logicalId": "ezaudit-lambda"
      },
      "lambda": {
        "memoryMb": 1024,
        "timeoutSec": 120,
        "runtime": "nodejs18.x",
        "payload": {
          "json": {
            "url": "https://facebook.com"
          }
        },
        "environmentVariables": {}
      }
    }
  ]
}
